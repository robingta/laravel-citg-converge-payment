{
    "name": "citg/laravel-converge-pay",
    "description": "Laravel Library for Converge Payment Gateway",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "converge",
        "payment",
        "gateway"
    ],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Robin Khan",
            "email": "robin@primtechs.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "CITG\\ConvergePay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CITG\\ConvergePay\\ConvergePayServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "dev-main"
    }
}
